<section id="logo-top">
    <div class="container">
        <!-- top nav starts -->
        <div class="top-nav">
            <div class="navlink"><a href="{{ linkUrl.dekaron }}" target="_blank">{{ 'goback' | translate }}</a></div>
            <div class="language-bar">
                <div class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown">
                        <span style="padding-right: 10px;"><img preload-image ng-src="{{imgUrl}}/flag-{{ flag }}.svg"
                                                                default-image="{{imgUrl}}/default.png" height="15"
                                                                alt="flag"></span><span>{{ lang }}</span><b
                            class="caret"></b>
                    </a>
                    <ul class="dropdown-menu">
                        <li ng-click="switchLanguage('en')">
                            <span><img preload-image ng-src="{{imgUrl}}/flag-uk.svg"
                                       default-image="{{imgUrl}}/default.png" height="15"
                                       alt="english flag"></span><span translate="{{ 'english' }}"></span>
                        </li>
                        <li ng-click="switchLanguage('de')" style="display:none">
                            <span><img preload-image ng-src="{{imgUrl}}/flag-germany.svg"
                                       default-image="{{imgUrl}}/default.png" height="15"
                                       alt="german flag"></span><span translate="{{ 'german' }}"></span>
                        </li>
                        <li ng-click="switchLanguage('fr')" style="display:none">
                            <span><img preload-image ng-src="{{imgUrl}}/flag-france.svg"
                                       default-image="{{imgUrl}}/default.png" height="15"
                                       alt="french flag"></span><span translate="{{ 'french' }}"></span>
                        </li>
                        <li ng-click="switchLanguage('pt')">
                            <span><img preload-image ng-src="{{imgUrl}}/flag-brazil.svg"
                                       default-image="{{imgUrl}}/default.png" height="15"
                                       alt="brazil flag"></span><span class="text-capitalize"
                                                                      translate="{{ 'portuguese' }}"></span>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- logo starts-->
        <div>
            <div class="logo-wrapper">
                <a href="{{ linkUrl.dekaron }}" target="_blank">
                    <ul>
                        <li><img preload-image ng-src="{{imgUrl}}/dk_logo.png" default-image="{{imgUrl}}/default.png"
                                 class="logo" alt="logo"/></li>
                        <li><img preload-image ng-src="{{imgUrl}}/update_copy.png"
                                 default-image="{{imgUrl}}/default.png" class="logo" alt="logo"/></li>
                    </ul>
                </a>
            </div>
        </div>
    </div>
</section>

